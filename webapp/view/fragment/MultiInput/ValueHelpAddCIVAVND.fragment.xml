<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">

		<TableSelectDialog
		noDataText="Nessun dato"
		app:filterField="Value"
		app:filterTableField="MWST"
		title="{i18n>lblMWST}"
		app:input="dialogAddSales6"
		search="_handleValueHelpSearch"
		confirm="handleValueHelpCloseCIVAVND"
		cancel="handleValueHelpCloseCIVAVND"
		items="{path : 'MatchCodesAddCIVAVND>/'}">
		<ColumnListItem vAlign="Middle">
			<cells>
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Name}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Value}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Name2}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Value2}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Name3}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Value3}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Value4}" />
				<ObjectIdentifier text="{MatchCodesAddCIVAVND>Value5}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column hAlign="Center">
				<header>
					<Text text="{i18n>lblMWST}"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>lblDescMWST}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
				<header>
					<Text text="{i18n>lblCodIVAV}"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>lblDescCodIVAV}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
				<header>
					<Text text="{i18n>lblCodIVAA}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>lblDescCodIVAA}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
				<header>
					<Text text="{i18n>lblReverse}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>lblDetrazione}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>

